{
  "mount": {
    "public": "/",
    "src/client": "/_dist_",
    "assets": "/assets"
  },
  "plugins": [
    ["@snowpack/plugin-babel", {
      "input": [".jsx"],
      "transformOptions": {
        "presets": ["@babel/preset-react"],
        "plugins": ["@babel/plugin-syntax-class-properties"]
      }
    }],
    ["@snowpack/plugin-react-refresh", {"babel":  false}],
    "@snowpack/plugin-dotenv"
  ]
}